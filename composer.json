{
  "name": "esase/tiny-service-manager",
  "type": "library",
  "description": "The Service Manager is a service/object locator, tasked with retrieving other objects",
  "license": "MIT",
  "keywords": [
    "esase",
    "tinyframework",
    "microservices",
    "framework",
    "lightweight",
    "servicemanager",
    "container",
    "servicelocator",
    "psr",
    "di"
  ],
  "support": {
  },
   "authors": [
        {
            "name": "Alex Ermashev",
            "email": "alexermashev@gmail.com"
        }
    ],
  "require": {
    "php": "^7.4 || ^8.0",
    "psr/container": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0 || ^10.0",
    "php-coveralls/php-coveralls": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "Tiny\\ServiceManager\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TinyTest\\ServiceManager\\": "test/"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
  },
  "minimum-stability": "dev",
  "prefer-stable" : true
}
